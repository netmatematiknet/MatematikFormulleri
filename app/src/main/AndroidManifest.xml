        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Formuller"></activity>
        <activity android:name=".FormulGoster"></activity>

        <activity android:name=".UygulamaUrunlerimiz"></activity>
        <activity android:name=".UygulamaWebview" android:theme="@style/AppTheme1.NoActionBar"></activity>
        <activity android:name=".Uygulamalar" android:theme="@style/AppTheme1.NoActionBar"></activity>
        <activity android:name=".UygulamaUrunSayfasi"></activity>

    </application>

</manifest>